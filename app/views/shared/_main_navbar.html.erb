<% #TODO fix the appearance of this section to appear as column in LG and row in MD and below %>
<div class="text-white">

  <%= nav_link_to "Dashboard", edit_user_registration_path,   class: "block no-underline py-2 px-4", active_class: "bg-yellow-500 text-black hover:text-primary-800", inactive_class: "text-white hover:text-black hover:bg-yellow-500" %>
  <%= nav_link_to "Letters of Credit", letter_of_credits_path,   class: "block no-underline py-2 px-4", active_class: "bg-yellow-500 text-black hover:text-primary-800", inactive_class: "text-white hover:text-black hover:bg-yellow-500" %>
  <%= nav_link_to "Profile", edit_user_registration_path,   class: "block no-underline py-2 px-4", active_class: "bg-yellow-500 text-black hover:text-primary-800", inactive_class: "text-white hover:text-black hover:bg-yellow-500" %>
  <%= nav_link_to "Password", edit_account_password_path,   class: "block no-underline py-2 px-4", active_class: "bg-yellow-500 text-black hover:text-primary-800", inactive_class: "text-white hover:text-black hover:bg-yellow-500" %>
  <%= nav_link_to "Accounts", user_connected_accounts_path, class: "block no-underline py-2 px-4", active_class: "bg-yellow-500 text-black hover:text-primary-800", inactive_class: "text-white hover:text-black hover:bg-yellow-500" if Devise.omniauth_configs.any? %>
